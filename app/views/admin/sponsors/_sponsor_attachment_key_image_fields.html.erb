<div class="key-image-field">
  <div class="form-row form-group">
    <div class="col-12 col-mb-6">
      <%= f.label :file, "File" %>
      <%= f.file_field :file %>
      <%= f.hidden_field :_destroy %>
      <%= link_to 'Delete', '#', class: 'remove_key_image_field' %>
      <%= image_tag f.object.file_url, class: "img-fluid" if f.object.file.present? %>
    </div>
  </div>
</div>
