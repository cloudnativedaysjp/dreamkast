<div class="container">
    <h2>新規登録</h2>
    <div class="row">
        <%= form_with model: @profile do |form| %>
        <div class="form-group row">
            <label for="inputEmail3" class="col-sm-2 col-form-label">Email</label>
            <div class="col-sm-10">
                <%= form.text_field :last_name %><%= form.text_field :first_name %> 
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-10">
                <%= form.text_field :occupation %>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-10">
                <%= form.text_field :company_name %>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-10">
                <%= form.text_field :company_email %>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-10">
                <%= form.text_field :company_address %>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-10">
                <%= form.text_field :company_tel %>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-10">
                <%= form.text_field :department %>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-10">
                <%= form.text_field :position %>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-10">
                <%= form.submit %>
            </div>
        </div>
        <% end %>
    </div>
</div>